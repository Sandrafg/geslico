            <div class="small-4  medium-2 large-2 columns">                    
                <label for="middle-label">Municipio:</label>
            </div>
            
            <div class="small-8  medium-2 large-2 columns">
                <label for="middle-label"><%= @t_sede.t_edificio.cMunicipio rescue nil %></label>
            </div>
        </div>

    </div>
  </li>

  <li class="accordion-item">
    <a href="#" class="accordion-title">Contacto</a>
    <div class="accordion-content" data-tab-content="" >      
        
        <div class="row">
            <div class="small-5  medium-2 large-1 columns">
                <label for="middle-label">Persona:</label>
            </div>
            <div class="small-7  medium-10 large-5 columns">
                <label for="middle-label"><%= @t_sede.cPersonaContacto %></label>
            </div>
        
            <div class="small-5  medium-2 large-1 columns">
                <label for="middle-label">Mail:</label>
            </div>

            <div class="small-7  medium-10 large-5 columns">
                <label for="middle-label"><%= @t_sede.ceMailContacto %></label>
            </div>
        </div>
        
        <div class="row">
            <div class="small-5  medium-2 large-1 columns">
                <label for="middle-label">Fijo:</label>
            </div>

            <div class="small-7  medium-4 large-2 columns">
                <label for="middle-label"><%= @t_sede.cTelefonoContacto %></label>
