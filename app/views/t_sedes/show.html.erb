<div class="row"> 
  <h3 class="columns ">Ver Sede</h3>
</div>

<div class="row">

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Código:</label>
    </div>

    <div class="small-8 medium-4 large-2 columns">
      <label for="middle-label"><%= @t_sede.nCodSede %></label>
    </div>

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Cabecera:</label>
    </div>
    
    <div class="small-8 medium-4 large-1 columns">
      <% if  @t_sede.nCodCampus.nil? %>
          <label for="middle-label">-</label>
      <% else %>
          <% if  @t_sede.nCodCampus == @t_sede.nCodSede  %>                      
              <label for="middle-label">SI</label>
          <% else %>
              <label for="middle-label">NO</label>
          <% end %>
      <% end %>
    </div>

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Criticidad:</label>
    </div>

    <div class="small-8 medium-4 large-2 columns">

      <% if @t_sede.nCriticidad.nil? %>
          <label for="middle-label">-</label>
      <% else %>
          <% if  @t_sede.nCriticidad == 0  %>                      
              <label for="middle-label">Normal</label>
          <% else %>
              <% if  @t_sede.nCriticidad == 1  %>
                <label for="middle-label">Urgente</label>
              <% else %>
                <% if  @t_sede.nCriticidad == 2  %>
                  <label for="middle-label">Crítico</label>
                <% end %>
              <% end %>      
          <% end %>
      <% end %>

    </div>

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Estado:</label>
    </div>
  
    <div class="small-8 medium-4 large-3 columns end">
        <label for="middle-label"><%= @t_sede.t_estados_sede.cDescripcion %></label>               
    </div>
    
 </div>

 <div class="row">

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Nombre:</label>
    </div>

    <div class="small-8 medium-4 large-7 columns end">      
      <label for="middle-label"><%= @t_sede.cNombre %></label>
    </div>

    <div class="small-4 medium-2 large-1 columns">
      <label for="middle-label">Distrito:</label>
    </div>

    <div class="small-8 medium-4 large-3 columns end">
      <label for="middle-label"><%= @t_sede.t_distrito.cDistrito %></label>
    </div>

</div>

<div class="row">
    <div class="small-4  medium-2 large-1 columns">
        <label for="middle-label">Dirección:</label>
     </div>

     <div class="small-8  medium-4 large-7 columns">
        <label for="middle-label"><%= @t_sede.cDireccion %></label>
     </div>

    <div class="small-4  medium-2 large-1 columns">
        <label for="middle-label">Cód.Postal:</label>
     </div>

     <div class="small-8  medium-4 large-3 columns">
        <label for="middle-label"><%= @t_sede.cCodPostal %></label>
     </div>
</div>

<div class="row">
     <div class="small-4  medium-2 large-1 columns">
       <label for="middle-label">DevicePool:</label>
     </div>

     <div class="small-8  medium-4 large-4 columns">
        <% if !@t_sede.cDevicePool.nil? %>
          <label for="middle-label"><%= @t_sede.t_device_pool.cDevicePool %></label>
        <% else %>
          <label for="middle-label">-</label>
        <% end %>
     </div>

     <div class="small-4  medium-2 large-1 columns">
        <label for="middle-label">Acrónimo:</label>
     </div>

     <div class="small-8  medium-4 large-2 columns">
        <% if  !@t_sede.nIdAcronimo.nil?  %>
          <label for="middle-label"><%= @t_sede.t_acronimos_sede_edr.cCodigo %></label>
        <% else %>
          <label for="middle-label">-</label>
        <% end %>
     </div>

     <div class="small-4  medium-2 large-1 columns">
       <label for="middle-label">Gestión:</label>
     </div>

     <div class="small-8  medium-4 large-3 columns end">

        <% if  @t_sede.nCriticidad.nil?  %>
          <label for="middle-label">-</label>
        <% else %>
            <% if  @t_sede.nCriticidad == 0  %>                      
                <label for="middle-label">Municipal</label>
            <% else %>
                <% if  @t_sede.nCriticidad == 1  %>
                  <label for="middle-label">Privada</label>
                <% else %>
                  <% if  @t_sede.nCriticidad == 2  %>
                    <label for="middle-label">Mixta</label>
                  <% else %>
                    <% if  @t_sede.nCriticidad == 3  %>
                      <label for="middle-label">Otras Administ</label>
                    <% end %>
                  <% end %>
                <% end %>      
            <% end %>
        <% end %>

     </div>
</div>

<div class="row margin-bottom">
     <div class="small-4  medium-2 large-1 columns">
       <label for="middle-label">Grupo:</label>
     </div>

     <div class="small-8  medium-4 large-4 columns">
        <% if  !@t_sede.nZona.nil?  %>
          <label for="middle-label"><%= @t_sede.t_zona.cGrupo %></label>
        <% else %>
          <label for="middle-label">-</label>
        <% end %>
     </div>

     <div class="small-4  medium-2 large-1 columns">
        <label for="middle-label">Zona:</label>
     </div>

     <div class="small-8  medium-4 large-2 columns">
        <% if  !@t_sede.nZona.nil?  %>
          <label for="middle-label"><%= @t_sede.t_zona.cZona %></label>
        <% else %>
          <label for="middle-label">-</label>
        <% end %>
     </div>

     <div class="small-4  medium-2 large-1 columns">
       <label for="middle-label">ToIp:</label>
     </div>

     <div class="small-8  medium-4 large-3 columns end">                 
        <% if  @t_sede.bSedeToIP.nil?  %>
          <label for="middle-label">-</label>
        <% else %>                  
          <% if  @t_sede.bSedeToIP == true  %>
            <label for="middle-label">SI</label>
          <% else %>
            <% if  @t_sede.bSedeToIP == false  %>
              <label for="middle-label">NO</label>
            <% end %>
          <% end %>                  
        <% end %>
     </div>
</div>

<!--GESTION DE BOTONES -->
<div class="row">
    <%= link_to '<< Volver', t_sedes_path, :class => "success button" %>        

    <% if can? :read, TSede %> 
        <%= link_to 'Editar', edit_t_sede_path(@t_sede), :class => "success button" %> 
    <% end %>
</div>


