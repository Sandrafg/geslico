<%= form_for @t_AcronimosSedeEdr do |f| %>
	
	<% if @t_AcronimosSedeEdr.errors.any? %>  
	    <div class="panel warning callout radius">      
	      <strong><%= t('activerecord.errors.template.header', :model => TAcronimosSedeEdr.model_name.human, :count => @t_AcronimosSedeEdr.errors.count) %></strong>
	      <ul>
	      <% @t_AcronimosSedeEdr.errors.full_messages.each do |message| %>
	        <li><%= message %></li>
	      <% end %>
	      </ul>
	    </div>
  	<% end %>

  	<div class="row">

	    <div class="small-3 medium-2 large-2 columns end">
	      <label for="middle-label">Código:</label>
	    </div>
	   
	    <div class="small-4 medium-3 large-2 columns end">            
        	<%= f.text_field :cCodigo %>
      	</div>

	</div>

	<div class="row">
	    <div class="small-3 medium-2 large-2 columns">
	      <label for="middle-label">Descripción:</label>
	    </div>
	     
	   	<div class="small-9 medium-10 large-10 columns">	      
	      <%= f.text_field :cDescripcion %>
	    </div>   
	</div>

	<br>
	
	<!-- BOTONES -->
  	<div class="row">
      	<%= link_to '<< Volver', t_acronimos_sede_edrs_path, :class => "success button" %>
    	<%= f.submit 'Guardar', class: 'success button' %>      	     
  	</div>

<% end %>